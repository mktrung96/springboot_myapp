	<queryString>
		<![CDATA[Select a.shm_shareholdercheckin_id ID,c.Code,c.Name,REGISTER_NO,c.qty_hold,b.qty_vote,
to_char(TYPE || ' - ' || FULLNAME) FULLNAME,nvl(QTYVOTEAUTO,0) +  nvl(QTYVOTEMANUAL,0) TOTAL_QTYVOTE
from shm_vote_candidate a
Inner join shm_shareholdercheckin b on a.shm_shareholdercheckin_id = b.shm_shareholdercheckin_id
Inner join shm_share_holder c on b.shm_shareholder_id = c.shm_share_holder_id
inner join shm_candidate d on d.shm_candidate_ID = a.shm_candidate_ID
Where a.isinvalid = 'N'
Union all
Select distinct a.shm_shareholdercheckin_id ID,c.Code,c.Name,REGISTER_NO,c.qty_hold,b.qty_vote,
to_char('KHÔNG HỢP LỆ') FULLNAME,-1 TOTAL_QTYVOTE
from shm_vote_candidate a
Inner join shm_shareholdercheckin b on a.shm_shareholdercheckin_id = b.shm_shareholdercheckin_id
Inner join shm_share_holder c on b.shm_shareholder_id = c.shm_share_holder_id
inner join shm_candidate d on d.shm_candidate_ID = a.shm_candidate_ID
Where a.isinvalid = 'Y'
order by Code]]>
	</queryString>
